{
  "realm": "claude-lists",
  "enabled": true,
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "roles": {
    "realm": [
      {
        "name": "web_user",
        "description": "Authenticated user role for PostgREST"
      },
      {
        "name": "web_admin",
        "description": "Admin role - can delete any comment"
      }
    ]
  },
  "defaultRoles": ["web_user"],
  "clientScopes": [
    {
      "name": "postgrest",
      "description": "PostgREST custom claims",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true"
      },
      "protocolMappers": [
        {
          "name": "postgrest-role",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "postgrest_role",
            "claim.name": "role",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "username",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "username",
            "claim.name": "preferred_username",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "given name",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "user.attribute": "firstName",
            "claim.name": "given_name",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "subject",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "config": {
            "user.attribute": "id",
            "claim.name": "sub",
            "jsonType.label": "String",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "claude-lists-web",
      "enabled": true,
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "redirectUris": [
        "http://localhost:8080/*",
        "http://127.0.0.1:8080/*",
        "https://localhost:8443/*",
        "https://127.0.0.1:8443/*",
        "http://192.168.0.189:8080/*",
        "https://192.168.0.189:8443/*"
      ],
      "webOrigins": [
        "*"
      ],
      "protocol": "openid-connect",
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "postgrest"
      ]
    }
  ],
  "users": [
    {
      "username": "alice",
      "email": "alice@example.com",
      "firstName": "Alice",
      "lastName": "Smith",
      "enabled": true,
      "emailVerified": true,
      "attributes": {
        "postgrest_role": ["web_user"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": ["web_user"]
    },
    {
      "username": "bob",
      "email": "bob@example.com",
      "firstName": "Bob",
      "lastName": "Jones",
      "enabled": true,
      "emailVerified": true,
      "attributes": {
        "postgrest_role": ["web_user"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": ["web_user"]
    },
    {
      "username": "charlie",
      "email": "charlie@example.com",
      "firstName": "Charlie",
      "lastName": "Brown",
      "enabled": true,
      "emailVerified": true,
      "attributes": {
        "postgrest_role": ["web_user"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": ["web_user"]
    },
    {
      "username": "admin",
      "email": "admin@example.com",
      "firstName": "Admin",
      "lastName": "User",
      "enabled": true,
      "emailVerified": true,
      "attributes": {
        "postgrest_role": ["web_admin"]
      },
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": ["web_admin"]
    }
  ]
}
